  "scripts": {
    "tibro-modules": "npm run clean && npm install perun-redux perun-components && npm run dev",
    "clean": "rimraf node_modules/perun-redux && rimraf node_modules/perun-components",
    "dev": "cp .pre-push-hook.tpl .git/hooks/pre-push && webpack-dev-server --mode=development --hot --inline --progress --colors --watch --history-api-fallback --port 8080",
    "vpndev": "cp .pre-push-hook.tpl .git/hooks/pre-push && webpack-dev-server --inline --hot --progress --colors --watch --compress --history-api-fallback --port 9090",
    "prod": "cross-env NODE_ENV=production webpack-dev-server --inline --hot --progress --colors --watch --display-error-details --display-cached --history-api-fallback --port 8080 --host 0.0.0.0",
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress --colors",
    "build-war": "./build_war.sh",
    "test": "standard --fix | snazzy",
    "lintcss": "stylelint naits/css/*.css --color | snazzy ",
    "lintcss2": "stylelint naits/themes/Simplex/*.css --color | snazzy ",
    "test2": "eslint --fix naits/js/ || exit 0",
    "test3": "standard --fix",
    "stats": "webpack --profile --json > compilation-stats.json"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "jquery": true
    },
    "ignore": [
      "naits/css",
      "naits/img",
      "naits/jqgrid",
